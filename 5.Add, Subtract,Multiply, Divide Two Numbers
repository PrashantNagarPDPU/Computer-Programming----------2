def add(a, b):
    print("Addition:", a + b)

def subtract(a, b):
    print("Subtraction:", a - b)

def multiply(a, b):
    print("Multiplication:", a * b)

def divide(a, b):
        print("Division:", a / b)

x = float(input("Enter first number: "))
y = float(input("Enter second number: "))


add(x, y)
subtract(x, y)
multiply(x, y)
divide(x, y)